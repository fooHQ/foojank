var app = cli.app({
    name: "cp",
    usage: "Copy files",
    action: func(c) {
        args := c.args()
        if len(args) < 2 {
            print("not enough arguments")
            return
        } else if len(args) > 2 {
            print ("too many arguments")
            return
        }

        src := args[0]
